{
  "name": "intelli-buddy-executor",
  "displayName": "IntelliBuddy Executor",
  "description": "Executes IntelliBuddy",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intelli-buddy-executor.processFile",
        "title": "IntelliBuddy: Processing"
      },
      {
        "command": "intelli-buddy-executor.processFileWithoutDiff",
        "title": "IntelliBuddy: Processing Without Diff"
      }
    ],
    "configuration": {
      "title": "IntelliBuddy",
      "properties": {
        "intellibuddy.showDiff": {
          "type": "boolean",
          "default": true,
          "description": "Shows git diffs between the original version and the processed one."
        },
        "intellibuddy.configAbsolutePath": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "You can pass an absolute path to the configuration file"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build:clear && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "build:clear": "rm -rf dist"
  },
  "author": {
    "name": "misbiheyv",
    "email": "mishagorkovenko1103@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "intelli-buddy": "^1.0.0-alpha.2"
  }
}
